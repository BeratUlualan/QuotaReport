{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "cluster": {
      "type": "object",
      "properties": {
        "address": {
          "type": "string"
        },
        "port": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "access_token": {
          "type": "string"
        }
      },
      "required": [
        "address",
        "port",
        "username",
        "password",
        "access_token"
      ]
    },
    "email": {
      "type": "object",
      "properties": {
        "from": {
          "type": "string"
        },
        "to": {
          "type": "string"
        },
        "login": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "server": {
          "type": "string"
        },
        "port": {
          "type": "integer"
        },
        "use": {
          "type": "string"
        }
      },
      "required": [
        "from",
        "to",
        "login",
        "password",
        "server",
        "port",
        "use"
      ]
    }
  },
  "required": [
    "cluster",
    "email"
  ]
}